{
  "project": "DX-Partners-DA-Test",
  "editUrlLabel": "Document Authoring",
  "editUrlPattern": "https://da.live/edit#/{{org}}/{{site}}{{pathname}}",
  "plugins": [
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true,
      "environments": [
        "any"
      ]
    },
    {
      "containerId": "tools",
      "title": "Check Schema",
      "id": "checkschema",
      "environments": [
        "prod"
      ],
      "event": "check-schema",
      "excludePaths": [
        "/tools**",
        "*.json"
      ]
    },
    {
      "containerId": "tools",
      "title": "Preflight",
      "id": "preflight",
      "environments": [
        "dev",
        "preview",
        "live"
      ],
      "event": "preflight"
    },
    {
      "containerId": "tools",
      "id": "locales",
      "title": "Locales",
      "environments": [
        "dev",
        "preview",
        "live"
      ],
      "isPalette": true,
      "passConfig": true,
      "passReferrer": true,
      "paletteRect": "top: auto; bottom: 25px; left: 75px; height: 388px; width: 360px;",
      "url": "https://milo.adobe.com/tools/locale-nav",
      "includePaths": [
        "**.docx**"
      ]
    },
    {
      "id": "da-locales",
      "title": "Locales",
      "environments": [
        "edit"
      ],
      "daLibrary": true,
